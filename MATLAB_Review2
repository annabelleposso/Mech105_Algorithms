% Given experimental data
t_exp = 10:10:60;
c_exp = [3.4 2.6 1.6 1.3 1.0 0.5];
e = 2.71828

% Expected function
t_func = linspace(0,80,141)
c_func = 4.84.* e.^(-0.34.* t_func)

% Plot
plot(t_exp,c_exp,'--dr')
plot(t_func,c_func,'--g')
legend('1','2','3','4','5','6')
xlabel('Time')
ylabel('Concentration')
